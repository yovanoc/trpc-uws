{
	"name": "trpc-uws",
	"version": "0.6.0",
	"description": "tRPC adapter for uWebSockets.js server",
	"repository": {
		"type": "git",
		"url": "https://github.com/yovanoc/trpc-uws"
	},
	"license": "MIT",
	"author": {
		"name": "Christopher YOVANOVITCH",
		"email": "yovano_c@outlook.com"
	},
	"type": "module",
	"main": "./lib/index.js",
	"files": [
		"lib/",
		"package.json",
		"LICENSE.md",
		"README.md"
	],
	"scripts": {
		"build": "tsup",
		"format": "prettier .",
		"lint": "eslint . .*js --max-warnings 0",
		"lint:knip": "knip",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"lint:package-json": "npmPkgJsonLint .",
		"lint:packages": "pnpm dedupe --check",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"prepare": "husky install",
		"should-semantic-release": "should-semantic-release --verbose",
		"test": "vitest",
		"tsc": "tsc"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"dependencies": {
		"@trpc/server": "11.0.0-next-beta.274",
		"uWebSockets.js": "uNetworking/uWebSockets.js#v20.41.0"
	},
	"devDependencies": {
		"@release-it/conventional-changelog": "^8.0.1",
		"@trpc/client": "11.0.0-next-beta.274",
		"@types/eslint": "^8.56.2",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@vitest/coverage-v8": "^1.2.2",
		"console-fail-test": "^0.2.3",
		"cspell": "^8.3.2",
		"eslint": "^8.56.0",
		"eslint-plugin-deprecation": "^2.0.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-jsdoc": "^48.0.6",
		"eslint-plugin-jsonc": "^2.13.0",
		"eslint-plugin-markdown": "^3.0.1",
		"eslint-plugin-n": "^16.6.2",
		"eslint-plugin-no-only-tests": "^3.1.0",
		"eslint-plugin-perfectionist": "^2.5.0",
		"eslint-plugin-regexp": "^2.2.0",
		"eslint-plugin-vitest": "^0.3.22",
		"eslint-plugin-yml": "^1.12.2",
		"husky": "^9.0.10",
		"jsonc-eslint-parser": "^2.4.0",
		"knip": "^5.0.0",
		"lint-staged": "^15.2.2",
		"markdownlint": "^0.33.0",
		"markdownlint-cli": "^0.39.0",
		"npm-package-json-lint": "^7.1.0",
		"npm-package-json-lint-config-default": "^6.0.0",
		"prettier": "^3.2.5",
		"prettier-plugin-curly": "^0.1.3",
		"prettier-plugin-packagejson": "^2.4.10",
		"release-it": "^17.0.3",
		"sentences-per-line": "^0.2.1",
		"should-semantic-release": "^0.2.1",
		"tsup": "^8.0.2",
		"typescript": "^5.3.3",
		"unws": "^0.2.4",
		"vitest": "^1.2.2",
		"yaml-eslint-parser": "^1.2.2",
		"zod": "^3.22.4"
	},
	"packageManager": "pnpm@8.15.1",
	"engines": {
		"node": ">=18"
	},
	"publishConfig": {
		"provenance": true
	}
}
